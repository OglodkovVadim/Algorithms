int binarySearch(std::vector<int>& arr, int vol) {
    int left = 0;
    int right = arr.size() - 1;
    while (left <= right) {
        int mid = left + (right - left) / 2;
        if (arr[mid] == vol) {
            return mid;
        } else if (arr[mid] < vol) {
            left = mid + 1;
        } else {
            right = mid - 1;
        }
    }
    return -1;
}
